# =============================================================================
# MoveX Frontend - Production Environment Configuration
# Copy this to the frontend project before building
# =============================================================================

# Application Environment
VITE_APP_ENV=production
VITE_DEBUG_MODE=false

# API Base URLs (via Nginx API Gateway on port 8080)
# Replace YOUR_SERVER_IP with actual server IP or domain
VITE_API_BASE_URL=http://YOUR_SERVER_IP:8080
VITE_AUTH_API_BASE_URL=http://YOUR_SERVER_IP:8080/api/auth
VITE_OMS_API_BASE_URL=http://YOUR_SERVER_IP:8080/api/oms
VITE_MASTERDATA_API_BASE_URL=http://YOUR_SERVER_IP:8080/api/master-data
VITE_TMS_API_BASE_URL=http://YOUR_SERVER_IP:8080/api/tms

# Prototype Mode disabled in production
VITE_PROTOTYPE_MODE=false
